{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "storage": {
      "type": "azure.bicep.v0",
      "path": "storage.module.bicep",
      "params": {
        "principalId": "",
        "principalType": ""
      }
    },
    "clustering": {
      "type": "value.v0",
      "connectionString": "{storage.outputs.tableEndpoint}"
    },
    "grainState": {
      "type": "value.v0",
      "connectionString": "{storage.outputs.blobEndpoint}"
    },
    "bank": {
      "type": "project.v1",
      "path": "../Bank/Bank.csproj",
      "deployment": {
        "type": "azure.bicep.v0",
        "path": "bank.module.bicep",
        "params": {
          "bank_containerport": "{bank.containerPort}",
          "storage_outputs_tableendpoint": "{storage.outputs.tableEndpoint}",
          "orleans_cluster_id_value": "{orleans-cluster-id.value}",
          "orleans_service_id_value": "{orleans-service-id.value}",
          "storage_outputs_blobendpoint": "{storage.outputs.blobEndpoint}",
          "outputs_azure_container_registry_managed_identity_id": "{.outputs.AZURE_CONTAINER_REGISTRY_MANAGED_IDENTITY_ID}",
          "outputs_managed_identity_client_id": "{.outputs.MANAGED_IDENTITY_CLIENT_ID}",
          "outputs_azure_container_apps_environment_id": "{.outputs.AZURE_CONTAINER_APPS_ENVIRONMENT_ID}",
          "outputs_azure_container_registry_endpoint": "{.outputs.AZURE_CONTAINER_REGISTRY_ENDPOINT}",
          "bank_containerimage": "{bank.containerImage}"
        }
      },
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{bank.bindings.http.targetPort}",
        "Orleans__Clustering__ProviderType": "AzureTableStorage",
        "Orleans__Clustering__ServiceKey": "clustering",
        "ConnectionStrings__clustering": "{clustering.connectionString}",
        "Orleans__ClusterId": "{orleans-cluster-id.value}",
        "Orleans__ServiceId": "{orleans-service-id.value}",
        "Orleans__EnableDistributedTracing": "true",
        "Orleans__GrainStorage__grainState__ProviderType": "AzureBlobStorage",
        "Orleans__GrainStorage__grainState__ServiceKey": "grainState",
        "ConnectionStrings__grainState": "{grainState.connectionString}",
        "Orleans__Endpoints__SiloPort": "{bank.bindings.orleans-silo.targetPort}",
        "Orleans__Endpoints__GatewayPort": "{bank.bindings.orleans-gateway.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "orleans-silo": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 8000
        },
        "orleans-gateway": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 8001
        }
      }
    },
    "scaler": {
      "type": "project.v1",
      "path": "../Scaler/Scaler.csproj",
      "deployment": {
        "type": "azure.bicep.v0",
        "path": "scaler.module.bicep",
        "params": {
          "scaler_containerport": "{scaler.containerPort}",
          "storage_outputs_tableendpoint": "{storage.outputs.tableEndpoint}",
          "orleans_cluster_id_value": "{orleans-cluster-id.value}",
          "orleans_service_id_value": "{orleans-service-id.value}",
          "outputs_azure_container_registry_managed_identity_id": "{.outputs.AZURE_CONTAINER_REGISTRY_MANAGED_IDENTITY_ID}",
          "outputs_managed_identity_client_id": "{.outputs.MANAGED_IDENTITY_CLIENT_ID}",
          "outputs_azure_container_apps_environment_id": "{.outputs.AZURE_CONTAINER_APPS_ENVIRONMENT_ID}",
          "outputs_azure_container_registry_endpoint": "{.outputs.AZURE_CONTAINER_REGISTRY_ENDPOINT}",
          "scaler_containerimage": "{scaler.containerImage}"
        }
      },
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{scaler.bindings.http.targetPort}",
        "Orleans__Clustering__ProviderType": "AzureTableStorage",
        "Orleans__Clustering__ServiceKey": "clustering",
        "ConnectionStrings__clustering": "{clustering.connectionString}",
        "Orleans__ClusterId": "{orleans-cluster-id.value}",
        "Orleans__ServiceId": "{orleans-service-id.value}",
        "Orleans__EnableDistributedTracing": "true"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http2"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http2"
        }
      }
    },
    "api": {
      "type": "project.v1",
      "path": "../API/API.csproj",
      "deployment": {
        "type": "azure.bicep.v0",
        "path": "api.module.bicep",
        "params": {
          "api_containerport": "{api.containerPort}",
          "storage_outputs_tableendpoint": "{storage.outputs.tableEndpoint}",
          "orleans_cluster_id_value": "{orleans-cluster-id.value}",
          "orleans_service_id_value": "{orleans-service-id.value}",
          "storage_outputs_blobendpoint": "{storage.outputs.blobEndpoint}",
          "outputs_azure_container_registry_managed_identity_id": "{.outputs.AZURE_CONTAINER_REGISTRY_MANAGED_IDENTITY_ID}",
          "outputs_managed_identity_client_id": "{.outputs.MANAGED_IDENTITY_CLIENT_ID}",
          "outputs_azure_container_apps_environment_id": "{.outputs.AZURE_CONTAINER_APPS_ENVIRONMENT_ID}",
          "outputs_azure_container_registry_endpoint": "{.outputs.AZURE_CONTAINER_REGISTRY_ENDPOINT}",
          "api_containerimage": "{api.containerImage}",
          "_scaler_bindings_http_url_": "{scaler.bindings.http.url}"
        }
      },
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{api.bindings.http.targetPort}",
        "Orleans__Clustering__ProviderType": "AzureTableStorage",
        "Orleans__Clustering__ServiceKey": "clustering",
        "ConnectionStrings__clustering": "{clustering.connectionString}",
        "Orleans__ClusterId": "{orleans-cluster-id.value}",
        "Orleans__ServiceId": "{orleans-service-id.value}",
        "Orleans__EnableDistributedTracing": "true",
        "Orleans__GrainStorage__grainState__ProviderType": "AzureBlobStorage",
        "Orleans__GrainStorage__grainState__ServiceKey": "grainState",
        "ConnectionStrings__grainState": "{grainState.connectionString}",
        "Orleans__Endpoints__SiloPort": "{api.bindings.orleans-silo.targetPort}",
        "Orleans__Endpoints__GatewayPort": "{api.bindings.orleans-gateway.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        },
        "orleans-silo": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 8000
        },
        "orleans-gateway": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "targetPort": 8001
        }
      }
    },
    "simulations": {
      "type": "project.v1",
      "path": "../Simulations/Simulations.csproj",
      "deployment": {
        "type": "azure.bicep.v0",
        "path": "simulations.module.bicep",
        "params": {
          "simulations_containerport": "{simulations.containerPort}",
          "storage_outputs_tableendpoint": "{storage.outputs.tableEndpoint}",
          "orleans_cluster_id_value": "{orleans-cluster-id.value}",
          "orleans_service_id_value": "{orleans-service-id.value}",
          "outputs_azure_container_apps_environment_default_domain": "{.outputs.AZURE_CONTAINER_APPS_ENVIRONMENT_DEFAULT_DOMAIN}",
          "outputs_azure_container_registry_managed_identity_id": "{.outputs.AZURE_CONTAINER_REGISTRY_MANAGED_IDENTITY_ID}",
          "outputs_managed_identity_client_id": "{.outputs.MANAGED_IDENTITY_CLIENT_ID}",
          "outputs_azure_container_apps_environment_id": "{.outputs.AZURE_CONTAINER_APPS_ENVIRONMENT_ID}",
          "outputs_azure_container_registry_endpoint": "{.outputs.AZURE_CONTAINER_REGISTRY_ENDPOINT}",
          "simulations_containerimage": "{simulations.containerImage}"
        }
      },
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{simulations.bindings.http.targetPort}",
        "Orleans__Clustering__ProviderType": "AzureTableStorage",
        "Orleans__Clustering__ServiceKey": "clustering",
        "ConnectionStrings__clustering": "{clustering.connectionString}",
        "Orleans__ClusterId": "{orleans-cluster-id.value}",
        "Orleans__ServiceId": "{orleans-service-id.value}",
        "Orleans__EnableDistributedTracing": "true",
        "services__api__http__0": "{api.bindings.http.url}",
        "services__api__https__0": "{api.bindings.https.url}",
        "services__api__orleans-silo__0": "{api.bindings.orleans-silo.url}",
        "services__api__orleans-gateway__0": "{api.bindings.orleans-gateway.url}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "orleans-cluster-id": {
      "type": "parameter.v0",
      "value": "{orleans-cluster-id.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "default": {
            "generate": {
              "minLength": 25,
              "upper": false,
              "special": false
            }
          }
        }
      }
    },
    "orleans-service-id": {
      "type": "parameter.v0",
      "value": "{orleans-service-id.inputs.value}",
      "inputs": {
        "value": {
          "type": "string",
          "default": {
            "generate": {
              "minLength": 25,
              "upper": false,
              "special": false
            }
          }
        }
      }
    }
  }
}